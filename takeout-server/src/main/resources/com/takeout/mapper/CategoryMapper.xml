<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.takeout.mapper.CategoryMapper">

    <select id="query" resultType="com.takeout.entity.Category">
        SELECT *
        FROM category
        WHERE type = #{type}
    </select>
</mapper>